<svg xmlns="http://www.w3.org/2000/svg" width="300" height="400" viewBox="0 0 300 400">
  <defs>
    <linearGradient id="bg5" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#8B5CF6;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#EC4899;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <rect width="300" height="400" fill="url(#bg5)"/>
  
  <!-- Group cosplay -->
  <g fill="white" opacity="0.8">
    <!-- Three figures -->
    <circle cx="100" cy="140" r="25"/>
    <circle cx="150" cy="130" r="25"/>
    <circle cx="200" cy="140" r="25"/>
    
    <rect x="80" y="165" width="40" height="60" rx="6"/>
    <rect x="130" y="155" width="40" height="60" rx="6"/>
    <rect x="180" y="165" width="40" height="60" rx="6"/>
    
    <!-- Arms linking -->
    <rect x="120" y="185" width="10" height="20" rx="5"/>
    <rect x="170" y="185" width="10" height="20" rx="5"/>
    
    <!-- Team banner -->
    <rect x="50" y="250" width="200" height="30" rx="4" fill="rgba(255,255,255,0.4)"/>
    <text x="150" y="270" font-family="Arial, sans-serif" font-size="14" font-weight="bold" text-anchor="middle" fill="#6B46C1">TEAM COSPLAY</text>
  </g>
  
  <text x="150" y="320" font-family="Arial, sans-serif" font-size="16" font-weight="bold" text-anchor="middle" fill="white">Group Performance</text>
  <text x="150" y="340" font-family="Arial, sans-serif" font-size="12" text-anchor="middle" fill="rgba(255,255,255,0.9)">Team Spirit</text>
</svg>