<svg xmlns="http://www.w3.org/2000/svg" width="400" height="200" viewBox="0 0 400 200">
  <defs>
    <linearGradient id="newsGrad2" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#F472B6;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#8B5CF6;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <rect width="400" height="200" fill="url(#newsGrad2)"/>
  
  <!-- Winners celebration design -->
  <g fill="white" opacity="0.9">
    <!-- Trophy -->
    <rect x="180" y="120" width="40" height="50" rx="3" fill="rgba(255,255,255,0.8)"/>
    <ellipse cx="200" cy="110" rx="25" ry="15" fill="rgba(255,255,255,0.9)"/>
    <rect x="190" y="95" width="20" height="15" rx="10" fill="rgba(255,255,255,0.7)"/>
    
    <!-- Winner podium -->
    <rect x="160" y="140" width="25" height="30" fill="rgba(255,255,255,0.6)"/>
    <rect x="190" y="130" width="25" height="40" fill="rgba(255,255,255,0.7)"/>
    <rect x="220" y="145" width="25" height="25" fill="rgba(255,255,255,0.5)"/>
    
    <!-- Confetti -->
    <circle cx="100" cy="50" r="3" fill="rgba(255,255,255,0.8)"/>
    <circle cx="120" cy="70" r="2" fill="rgba(255,255,255,0.6)"/>
    <circle cx="280" cy="45" r="4" fill="rgba(255,255,255,0.7)"/>
    <circle cx="300" cy="65" r="2" fill="rgba(255,255,255,0.5)"/>
    <circle cx="320" cy="80" r="3" fill="rgba(255,255,255,0.6)"/>
  </g>
  
  <text x="200" y="40" font-family="Arial, sans-serif" font-size="18" font-weight="bold" text-anchor="middle" fill="white">CONGRATULATIONS</text>
  <text x="200" y="60" font-family="Arial, sans-serif" font-size="14" text-anchor="middle" fill="rgba(255,255,255,0.9)">Winners Announced</text>
</svg>